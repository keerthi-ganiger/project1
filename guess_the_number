import random
cinp=random.randint(10,500)
print("GAME INSTRUCTIONS\n In this game computer sets anumber andyou should enter a number between 100-200\n and guess the no given by computer ")
for i in range(10):
    inp=int(input("Enter number"))
    if inp==cinp:
        print("You got it correct\n the number was",cinp)
        break;
    else:
        for i in range(1):
            if cinp<inp:
                print("your guess was quite high")
            else:
                print("your guess was quite low")
                
                
